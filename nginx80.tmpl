server {

      root /var/www/html;

      # Add index.php to the list if you are using PHP
      index index.html index.htm index.nginx-debian.html index.php;

      server_name www.virtualmin.indinet.es virtualmin.indinet.es;
      client_max_body_size 0;
      location / {
#        return 301 https://10.10.10.102$request_uri;
        proxy_pass https://10.10.10.102:10000;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection 'upgrade';
        proxy_set_header Host $host;
        proxy_cache_bypass $http_upgrade;

        #include /etc/nginx/proxy_params;
    }

    listen [::]:443 ssl; # managed by Certbot
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/virtualmin.indinet.es/fullchain.pem; # managed by Certbot
 # managed by Certb$
    ssl_certificate_key /etc/letsencrypt/live/virtualmin.indinet.es/privkey.pem; # managed by Certbot
 # managed by Cer$
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}


 server {
    if ($host = virtualmin.indinet.es) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
      listen 80;
      listen [::]:80;
      server_name www.virtualmin.indinet.es virtualmin.indinet.es;
    return 404; # managed by Certbot
}
